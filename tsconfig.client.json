{
  "exclude": [
    "server"
  ],
  "compilerOptions": {
    "module": "NodeNext",
    "target": "es5",
    "jsx": "react-jsx",
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": true,
    "strictNullChecks": true,
    "noImplicitAny": true
  }
}